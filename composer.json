{
  "name": "kronos/rrule",
  "type": "library",
  "description": "A simple object oriented rrule parser",
  "keywords": ["rrule", "rfc2445"],
  "homepage": "https://github.com/kronostechnologies/rrule",
  "license": "MIT",
  "authors": [
    {
      "name": "Kronos Technologies",
      "email": "sysadmin@kronostechnologies.com",
      "homepage": "https://www.kronos.ca/",
      "role": "Devops"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "vimeo/psalm": "5.21.1",
    "phpunit/phpunit": "10.5.9",
    "cyclonedx/cyclonedx-php-composer": "5.0.0",
    "squizlabs/php_codesniffer": "3.8.1",
    "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
    "kronostechnologies/php-coding-standard": "2.0.0"
  },
  "repositories": [
    { "type": "git", "url": "https://github.com/kronostechnologies/rrule" }
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\": "src/Kronos/",
      "Kronos\\Tests\\": "tests/Kronos"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "cyclonedx/cyclonedx-php-composer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
